{
  "name": "generate-webtask",
  "description": "Generate a starting point for a webtask.io service.",
  "version": "0.1.0",
  "homepage": "https://github.com/generate/generate-webtask",
  "author": "Brian Woodward (https://github.com/doowb)",
  "repository": "generate/generate-webtask",
  "bugs": {
    "url": "https://github.com/generate/generate-webtask/issues"
  },
  "license": "MIT",
  "files": [
    "generator.js",
    "index.js",
    "LICENSE",
    "templates"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "test": "mocha"
  },
  "dependencies": {
    "generate-defaults": "^0.6.0",
    "is-valid-app": "^0.2.0",
    "through2": "^2.0.1"
  },
  "devDependencies": {
    "bdd-stdin": "^0.2.0",
    "delete": "^0.3.2",
    "generate": "^0.11.2",
    "gulp-format-md": "^0.1.10",
    "mocha": "^3.0.2",
    "npm-install-global": "^0.1.2",
    "verb-generate-readme": "^0.1.30",
    "verb-trees": "^0.1.0"
  },
  "keywords": [
    "generate",
    "webtask"
  ],
  "verb": {
    "toc": {
      "method": "preWrite",
      "maxdepth": 3,
      "footer": "\n\n_(TOC generated by [verb](https://github.com/verbose/verb) using [markdown-toc](https://github.com/jonschlinkert/markdown-toc))_",
      "render": true
    },
    "layout": "generator",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "related": {
      "list": [
        "generate"
      ]
    },
    "reflinks": [
      "assemble",
      "base",
      "generate",
      "gulp"
    ],
    "lint": {
      "reflinks": true
    }
  }
}
